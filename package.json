{
  "name": "case-angular-library-wrapper",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build case-angular-library --configuration=development",
    "build:prod": "ng build case-angular-library --configuration=production",
    "build:watch": " ng build case-angular-library --configuration=development --watch",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "publish": "ng build case-angular-library --configuration=production && cd dist/case-angular-library && npm pack && npm publish --access public"
  },
